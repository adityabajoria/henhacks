# Unit Tests for DateFormat.py
from bakery import assert_equal
assert_equal(date_value(Date(1,1,2023)), 20230101)
assert_equal(date_value(Date(0, 0, 2022)), 20220000)
assert_equal(date_value(Date(12, 31, 2023)), 20231231)
assert_equal(date_value(Date(1, 1, 2024)), 20240101)
assert_equal(date_value(Date(2, 1, 2024)), 20240201)
assert_equal(date_value(Date(12, 31, 2018)), 20181231)
assert_equal(date_value(Date(1, 1, 2019)), 20190101)
assert_equal(date_value(Date(12, 25, 2019)), 20191225)
